	<script type="text/javascript">
		let click_count = 0;
		var stateoftoggle = true;
		let modelsquery = '';

		function whenModelChoosen(model_id, sel, mark) {
			let model_name = sel.options[sel.selectedIndex].text;
			
  			location.replace("/extent-poliuritan-catalog.html?poliurit="+mark+"&model_id="+model_id+"&model_name="+model_name);
		}

		$(function() {
			$('#toggle-event').change(function() {
				stateoftoggle = $(this).prop('checked');
				document.getElementById('resSearch').innerHTML='';
				if(stateoftoggle == false){ // searching by name of part
					document.getElementById("markaselect").disabled = false;
					document.getElementById("search").value = '';
					document.getElementById("search").disabled = true;
					document.getElementById("modelselect").innerHTML = "<option>-<option>";
					}else{ // searching by OEN
					document.getElementById("search").value = '';
					document.getElementById("search").disabled = false;
					document.getElementById("modelselect").innerHTML = "<option>-<option>";
					document.getElementById("markaselect").disabled = true;
					document.getElementById("markaselect").value = 0;
					document.getElementById("modelselect").disabled = true;
				}
			});


			$('#markaselect').change(function() {
				if(document.getElementById("markaselect").value != 0){
					document.getElementById("modelselect").disabled = false;
					modelsquery = this.value;

					$.ajax({
						type: "POST",
						url: "ajax/get_models_list.php?action=sf",
						data: {"automarka": modelsquery},
						cache: false,
						success: function(response2){
							$("#modelselect").html(response2);
						}
					});
				}
				else{
					document.getElementById("modelselect").disabled = true;
				}
			});

			$('#modelselect').change(function() {
				document.getElementById("search").disabled = false;
			});


		});


		$(document).ready(function(){  /*worning window show*/
			let doc_widt = $(document).width();
			if(doc_widt < 424 ){
				$('.warning').html("<a>ВНИМАНИЕ</a>!");
			}
			if(click_count==0){
				$(".warning").click(function() {
					$(this).html("Если вы не нашли нужной вам позиции, позвоните менеджеру, т.к. в каталоге представленны не все позиции, которые есть в наличии! <span onClick=\"hidediv()\" class=\"glyphicon glyphicon-remove-circle\" aria-hidden=\"true\"></span>");
					click_count = 1;
				});
			}


		});

		function hidediv() {	//alert(click_count);
			//$('div.warning').remove();
			$.ajax({
					type: "GET",
					url: "ajax/sessioChang.php",
					data: {"tiptool": 'off'},
					cache: false,
					success:  function(response){
						$(".alert").html(response);
					}
			});
		};


		$('#myModal').on('shown.bs.modal', function (event) {  /*modal window open*/
			data = $( event.relatedTarget ).data( "text" );
			$( "#result" ).html( data );
			$( "#crossinp" ).attr( "value",data2 );
		});

		function Goid(id) {
			data2 = $("#crosses"+id).text();
			$( "#crossinp" ).attr( "value",data2 );
		}

		document.getElementById("search").oninput = function(){
			if(stateoftoggle == false){

				let searchphrase = document.getElementById("search").value;

				if(searchphrase.length>1){
					let div = document.getElementById("resSearch");
					var re = new RegExp(this.value,'i'), text="";
					for(var i = 0; i < arr.length; i++){
						if(re.test(arr[i])) text+=arr[i]+"<br>";
					}
					div.innerHTML=text;
					choosDiv(div);
					strarray = this.value.split(' ');
					if(strarray.length == 3){

					}
				}

				arr=['<div class="pointer">сайлентблок передний</div>',
				'<div class="pointer">сайлентблок задний</div>',
				'<div class="pointer">втулка стабилизатора</div>',
				'<div class="pointer">втулка рессоры</div>',
				'<div class="pointer">подушка двигателя</div>',
				'<div class="pointer">подушка кпп</div>',
				'<div class="pointer">подушка балки</div>',
				'<div class="pointer">опора амортизатора</div>',
				'<div class="pointer">манжет</div>',
				'<div class="pointer">пыльник шруса</div>'
				];

				function choosDiv(div){
					var selectedDiv;
					div.onclick = function(event) {
						let target = event.target;
						let phase =  target.textContent;
						$("#main_conteiner").html("<center><img src=\"images/preloader2.gif\" /></center>");
						setTimeout(backInInput, 1500, phase, div);
						//backInInput(phase, div);
					}
				}
				function backInInput(name, searchdiv){
					document.getElementById("search").value = name;
					let el = document.getElementById("search");
					let selected_model = document.getElementById("modelselect").value;
					el.focus();
					searchdiv.innerHTML='';
					$.ajax({
						type: "GET",
						url: "ajax/search.php?action=by_name",
						data: {"search": name, "selmodel": selected_model},
						cache: false,
						success: function(response){
							$("#main_conteiner").html(response);
						}
					});
				}
			}
			else{
				var search = $("#search").val();  //search if toggle on OEN searching
				var box=$(this).val();
				if(box.length >= 5)
				{
					$.ajax({
						type: "GET",
						url: "ajax/search.php?action=by_oen",
						data: {"search": search},
						cache: false,
						success: function(response){
							$("#resSearch").html(response);
						}
					});
				}
			}
		}



	var clicksCounter = 0;
	var allBlocks = document.getElementsByClassName('suspention-block');
	for (let ll = 0; ll < allBlocks.length; ll++) {
			allBlocks[ll].addEventListener("click", function(e){
            clicksCounter++;
			x = e.target;
			//parentNode = e.path[1];
			parentDiv = x.parentNode;
			let cod_from_src = parentDiv.lastElementChild.src.split(/(\\|\/)/g).pop();
			cod_from_src = cod_from_src.slice(0, -4);
			//console.log(parentNode.childElementCount);
			cod_from_src = cod_from_src.split('_');
			cod_from_src.shift();

			for(let ind = 0; parentDiv.childElementCount > ind; ind++){
				if(parentDiv.childNodes[ind] == x){
					cod_from_src_main = cod_from_src[ind];
					break;
				}
			}
			//console.log(cod_from_src_main);
			//cod_from_src = cod_from_src.join();

			let parentDivList = '';
			let parentDivItem = '';
			let class_row_elem = [];

			class_row_elem = document.getElementsByClassName("prod-row");

			for(let z = 0; z < class_row_elem.length; z++){
				let value_of_code_div = class_row_elem[z].innerText;

				if(value_of_code_div.includes(cod_from_src_main)){
					parentDivList =  document.getElementById('products_list');
					parentDivItem =  class_row_elem[z].parentNode;
					break;
				}

			}

			if(parentDivItem != ''){ // if this product is on the page
				var top_div_element = parentDivItem.cloneNode(true);
				parentDivItem.remove(); // remove existing div of products list
				parentDivList.insertBefore(top_div_element, parentDivList.firstChild);
				top_div_element.setAttribute('id','appear'); //put new on the top
                //if size of window

                if(clicksCounter>1){top_div_element.nextSibling.style.boxShadow = "0px 0px 0px 0px";}
                top_div_element.scrollIntoView({ behavior: 'smooth' });  // Top
                setTimeout(function() {
					top_div_element.style.opacity = '1';
					top_div_element.style.boxShadow = "inset 0px -1px 13px 0px #5dd502";
				}, 500);

			}

		});
			//return false;
	}

	</script>
